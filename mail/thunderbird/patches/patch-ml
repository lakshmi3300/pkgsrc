 ifeq ($(OS_ARCH),FreeBSD)
 EXTRA_LIBS	+= -pthread
 endif
+ifeq ($(OS_ARCH),DragonFly)
+EXTRA_LIBS	+= -pthread
+endif
 ifeq ($(OS_ARCH),Linux)
